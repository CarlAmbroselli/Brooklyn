#!/bin/bash

function finish() {
	f="${1}"

	# No control fd: bail out
	[[ -z "${f}" || -z "${CYDIA}" ]] && return
	cydia=(${CYDIA})

	# Cydia control fd version != 1: bail out
	[[ ${cydia[1]} -eq 1 ]] || return

	echo "finish:${f}" >&${cydia[0]}
}

echo Killing MobileSMS...
killall -9 MobileSMS

echo Killing imagent...
killall -9 imagent

uicache

finish reload

exit 0